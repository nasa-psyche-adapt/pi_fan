import board
from time import sleep


# GPIO.setmode(GPIO.BOARD)      # need this?

import RPi.GPIO as GPIO        # need this?



# CODE FOR LED

fan = 26

GPIO.setmode(GPIO.BCM) 			# initializes that 26 is the GPIO26 pin
GPIO.setup(fan, GPIO.OUT)


while True:
	
	GPIO.output(fan, GPIO.HIGH)
	print("Fan on")
	
GPIO.cleanup()
	
